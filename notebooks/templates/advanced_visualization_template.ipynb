    "## 5. Interactive Plotly Visualizations"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Interactive Revenue Dashboard with Plotly\n",
    "fig = make_subplots(\n",
    "    rows=3, cols=2,\n",
    "    subplot_titles=('Revenue Time Series', 'Regional Performance', \n",
    "                   'Profit Margin Distribution', 'Customer Conversion Funnel',\n",
    "                   'Revenue vs Costs Analysis', 'Product Category Breakdown'),\n",
    "    specs=[[{\"secondary_y\": True}, {\"type\": \"bar\"}],\n",
    "           [{\"type\": \"histogram\"}, {\"type\": \"funnel\"}],\n",
    "           [{\"type\": \"scatter\"}, {\"type\": \"pie\"}]],\n",
    "    vertical_spacing=0.08\n",
    ")\n",
    "\n",
    "# 1. Revenue Time Series with Moving Average\n",
    "fig.add_trace(\n",
    "    go.Scatter(x=business_data['date'], y=business_data['revenue'], \n",
    "              mode='lines', name='Daily Revenue', \n",
    "              line=dict(color='blue', width=1)),\n",
    "    row=1, col=1\n",
    ")\n",
    "\n",
    "# Add 7-day moving average\n",
    "ma_7 = business_data['revenue'].rolling(window=7).mean()\n",
    "fig.add_trace(\n",
    "    go.Scatter(x=business_data['date'], y=ma_7, \n",
    "              mode='lines', name='7-Day MA', \n",
    "              line=dict(color='red', width=2)),\n",
    "    row=1, col=1\n",
    ")\n",
    "\n",
    "# Add costs on secondary y-axis\n",
    "fig.add_trace(\n",
    "    go.Scatter(x=business_data['date'], y=business_data['costs'], \n",
    "              mode='lines', name='Costs', \n",
    "              line=dict(color='orange', width=1, dash='dash'),\n",
    "              yaxis='y2'),\n",
    "    row=1, col=1, secondary_y=True\n",
    ")\n",
    "\n",
    "# 2. Regional Performance Bar Chart\n",
    "regional_data = business_data.groupby('region').agg({\n",
    "    'revenue': 'mean',\n",
    "    'customers': 'mean'\n",
    "}).round(0)\n",
    "\n",
    "fig.add_trace(\n",
    "    go.Bar(x=regional_data.index, y=regional_data['revenue'], \n",
    "           name='Avg Revenue', marker_color='lightblue',\n",
    "           text=regional_data['revenue'], textposition='auto'),\n",
    "    row=1, col=2\n",
    ")\n",
    "\n",
    "# 3. Profit Margin Distribution\n",
    "fig.add_trace(\n",
    "    go.Histogram(x=business_data['profit_margin'], nbinsx=20, \n",
    "                name='Profit Margin', marker_color='green', opacity=0.7),\n",
    "    row=2, col=1\n",
    ")\n",
    "\n",
    "# 4. Customer Conversion Funnel\n",
    "funnel_data = {\n",
