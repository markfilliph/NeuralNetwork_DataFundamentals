    "ax9.axis('off')\n",
    "\n",
    "# Calculate key metrics\n",
    "total_revenue = business_data['revenue'].sum()\n",
    "avg_margin = business_data['profit_margin'].mean()\n",
    "total_customers = business_data['customers'].sum()\n",
