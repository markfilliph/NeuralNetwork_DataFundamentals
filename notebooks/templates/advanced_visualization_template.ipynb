   "source": [
    "# Comprehensive Business Dashboard\n",
    "fig = plt.figure(figsize=(20, 16))\n",
    "gs = fig.add_gridspec(4, 4, hspace=0.3, wspace=0.3)\n",
    "\n",
    "# 1. Revenue Time Series (spans 2 columns)\n",
    "ax1 = fig.add_subplot(gs[0, :2])\n",
    "ax1.plot(business_data['date'], business_data['revenue'], linewidth=2, color=BUSINESS_COLORS[0])\n",
    "ax1.fill_between(business_data['date'], business_data['revenue'], alpha=0.3, color=BUSINESS_COLORS[0])\n",
    "ax1.set_title('Revenue Trend Over Time', fontsize=14, fontweight='bold')\n",
    "ax1.set_ylabel('Revenue ($)')\n",
    "ax1.grid(True, alpha=0.3)\n",
    "ax1.xaxis.set_major_formatter(mdates.DateFormatter('%b'))\n",
    "ax1.xaxis.set_major_locator(mdates.MonthLocator(interval=2))\n",
    "\n",
    "# 2. Profit Margin Gauge\n",
    "ax2 = fig.add_subplot(gs[0, 2])\n",
    "avg_margin = business_data['profit_margin'].mean()\n",
    "colors = ['red', 'orange', 'yellow', 'lightgreen', 'green']\n",
    "wedges, texts = ax2.pie([20, 20, 20, 20, 20], colors=colors, startangle=180, \n",
    "                       counterclock=False, wedgeprops={'width': 0.3})\n",
    "# Add needle\n",
    "angle = 180 - (avg_margin / 50 * 180)  # Assuming 0-50% range\n",
    "ax2.annotate('', xy=(0.7*np.cos(np.radians(angle)), 0.7*np.sin(np.radians(angle))), \n",
    "            xytext=(0, 0), arrowprops=dict(arrowstyle='->', lw=3, color='black'))\n",
    "ax2.set_title(f'Avg Profit Margin\\n{avg_margin:.1f}%', fontsize=12, fontweight='bold')\n",
    "ax2.set_ylim(-0.1, 1)\n",
    "\n",
    "# 3. Regional Performance\n",
    "ax3 = fig.add_subplot(gs[0, 3])\n",
    "regional_revenue = business_data.groupby('region')['revenue'].mean().sort_values(ascending=True)\n",
    "bars = ax3.barh(regional_revenue.index, regional_revenue.values, color=BUSINESS_COLORS)\n",
    "ax3.set_title('Avg Revenue by Region', fontsize=12, fontweight='bold')\n",
    "ax3.set_xlabel('Average Revenue ($)')\n",
    "# Add value labels\n",
    "for i, bar in enumerate(bars):\n",
    "    width = bar.get_width()\n",
    "    ax3.text(width, bar.get_y() + bar.get_height()/2, f'${width:.0f}', \n",
    "            ha='left', va='center', fontsize=10)\n",
    "\n",
    "# 4. Customer Acquisition Trend\n",
    "ax4 = fig.add_subplot(gs[1, :2])\n",
    "monthly_customers = business_data.groupby('month')['customers'].mean()\n",
    "ax4.bar(monthly_customers.index, monthly_customers.values, color=BUSINESS_COLORS[1], alpha=0.8)\n",
    "ax4.set_title('Average Monthly Customer Acquisition', fontsize=14, fontweight='bold')\n",
    "ax4.set_xlabel('Month')\n",
    "ax4.set_ylabel('Customers')\n",
    "ax4.grid(True, alpha=0.3, axis='y')\n",
    "# Add trend line\n",
    "z = np.polyfit(monthly_customers.index, monthly_customers.values, 1)\n",
    "p = np.poly1d(z)\n",
    "ax4.plot(monthly_customers.index, p(monthly_customers.index), \"r--\", alpha=0.8, linewidth=2)\n",
    "\n",
    "# 5. Conversion Rate Distribution\n",
    "ax5 = fig.add_subplot(gs[1, 2])\n",
    "ax5.hist(business_data['conversion_rate'], bins=20, color=BUSINESS_COLORS[2], alpha=0.7, edgecolor='black')\n",
    "ax5.axvline(business_data['conversion_rate'].mean(), color='red', linestyle='--', \n",
    "           label=f'Mean: {business_data[\"conversion_rate\"].mean():.1f}%')\n",
    "ax5.set_title('Conversion Rate Distribution', fontsize=12, fontweight='bold')\n",
    "ax5.set_xlabel('Conversion Rate (%)')\n",
    "ax5.set_ylabel('Frequency')\n",
    "ax5.legend()\n",
    "ax5.grid(True, alpha=0.3)\n",
    "\n",
    "# 6. Product Category Performance\n",
    "ax6 = fig.add_subplot(gs[1, 3])\n",
    "category_performance = business_data.groupby('product_category')['profit_margin'].mean()\n",
    "wedges, texts, autotexts = ax6.pie(category_performance.values, labels=category_performance.index, \n",
    "                                  autopct='%1.1f%%', colors=BUSINESS_COLORS)\n",
    "ax6.set_title('Profit Margin by Category', fontsize=12, fontweight='bold')\n",
    "\n",
    "# 7. Revenue vs Costs Scatter\n",
    "ax7 = fig.add_subplot(gs[2, :2])\n",
    "scatter = ax7.scatter(business_data['costs'], business_data['revenue'], \n",
