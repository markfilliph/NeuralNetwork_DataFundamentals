   "source": [
    "## 2. Sample Data Generation"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Generate comprehensive sample dataset for visualization demos\n",
    "np.random.seed(42)\n",
    "n_samples = 500\n",
    "\n",
    "# Time series data\n",
    "dates = pd.date_range('2023-01-01', periods=365, freq='D')\n",
    "base_trend = np.linspace(100, 150, 365)\n",
    "seasonal = 10 * np.sin(2 * np.pi * np.arange(365) / 365 * 4)  # Quarterly seasonality\n",
    "noise = np.random.normal(0, 5, 365)\n",
    "time_series_values = base_trend + seasonal + noise\n",
    "\n",
    "# Business metrics dataset\n",
    "business_data = pd.DataFrame({\n",
    "    'date': dates,\n",
    "    'revenue': time_series_values,\n",
    "    'costs': time_series_values * 0.7 + np.random.normal(0, 3, 365),\n",
    "    'profit_margin': (time_series_values - time_series_values * 0.7) / time_series_values * 100,\n",
    "    'customers': np.random.poisson(50, 365) + 100,\n",
    "    'conversion_rate': np.random.beta(2, 8, 365) * 100,  # 0-100%\n",
    "    'region': np.random.choice(['North', 'South', 'East', 'West'], 365),\n",
    "    'product_category': np.random.choice(['Electronics', 'Clothing', 'Home', 'Sports'], 365),\n",
    "    'quarter': pd.to_datetime(dates).quarter,\n",
    "    'month': pd.to_datetime(dates).month\n",
    "})\n",
    "\n",
    "# Cross-sectional analysis data\n",
    "analysis_data = pd.DataFrame({\n",
    "    'id': range(n_samples),\n",
    "    'category_a': np.random.choice(['Type_1', 'Type_2', 'Type_3', 'Type_4'], n_samples),\n",
    "    'category_b': np.random.choice(['Group_X', 'Group_Y', 'Group_Z'], n_samples),\n",
    "    'numeric_1': np.random.normal(50, 15, n_samples),\n",
    "    'numeric_2': np.random.exponential(2, n_samples),\n",
    "    'numeric_3': np.random.uniform(0, 100, n_samples),\n",
    "    'score': np.random.beta(2, 5, n_samples) * 100,\n",
    "    'performance': np.random.gamma(2, 2, n_samples) * 10,\n",
    "    'satisfaction': np.random.normal(7, 1.5, n_samples).clip(1, 10)\n",
    "})\n",
    "\n",
    "# Add correlated target variable\n",
    "analysis_data['target'] = (\n",
    "    0.3 * analysis_data['numeric_1'] + \n",
    "    0.2 * analysis_data['score'] + \n",
    "    0.1 * analysis_data['performance'] +\n",
    "    np.random.normal(0, 5, n_samples)\n",
    ")\n",
    "\n",
    "print(f\"üìä Generated business time series: {len(business_data)} daily records\")\n",
    "print(f\"üìà Generated analysis dataset: {len(analysis_data)} samples\")\n",
    "print(f\"üìÖ Date range: {business_data['date'].min()} to {business_data['date'].max()}\")\n",
    "\n",
    "# Display sample data\n",
    "print(\"\\nüîç Business Data Sample:\")\n",
    "display(business_data.head())\n",
    "\n",
    "print(\"\\nüîç Analysis Data Sample:\")\n",
    "display(analysis_data.head())"
   ]
  },
