   "source": [
    "## 3. Statistical Visualizations"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Distribution Analysis Suite\n",
    "fig, axes = plt.subplots(2, 3, figsize=(18, 12))\n",
    "fig.suptitle('Statistical Distribution Analysis', fontsize=16, y=0.98)\n",
    "\n",
    "# 1. Histogram with KDE\n",
    "sns.histplot(data=analysis_data, x='numeric_1', kde=True, ax=axes[0,0])\n",
    "axes[0,0].set_title('Distribution with Kernel Density')\n",
    "axes[0,0].grid(True, alpha=0.3)\n",
    "\n",
    "# 2. Box plots by category\n",
    "sns.boxplot(data=analysis_data, x='category_a', y='score', ax=axes[0,1])\n",
    "axes[0,1].set_title('Score Distribution by Category')\n",
    "axes[0,1].tick_params(axis='x', rotation=45)\n",
    "axes[0,1].grid(True, alpha=0.3)\n",
    "\n",
    "# 3. Violin plots\n",
    "sns.violinplot(data=analysis_data, x='category_b', y='performance', ax=axes[0,2])\n",
    "axes[0,2].set_title('Performance Distribution (Violin Plot)')\n",
    "axes[0,2].grid(True, alpha=0.3)\n",
    "\n",
    "# 4. Correlation heatmap\n",
    "numeric_cols = ['numeric_1', 'numeric_2', 'numeric_3', 'score', 'performance', 'satisfaction', 'target']\n",
    "correlation_matrix = analysis_data[numeric_cols].corr()\n",
    "sns.heatmap(correlation_matrix, annot=True, cmap='RdBu_r', center=0, \n",
    "            square=True, ax=axes[1,0], cbar_kws={'shrink': 0.8})\n",
    "axes[1,0].set_title('Correlation Matrix')\n",
    "\n",
    "# 5. Scatter plot with regression\n",
    "sns.scatterplot(data=analysis_data, x='score', y='target', hue='category_a', ax=axes[1,1])\n",
    "sns.regplot(data=analysis_data, x='score', y='target', scatter=False, ax=axes[1,1], color='red')\n",
    "axes[1,1].set_title('Score vs Target (with Regression Line)')\n",
    "axes[1,1].grid(True, alpha=0.3)\n",
    "\n",
    "# 6. Distribution comparison\n",
    "for category in analysis_data['category_a'].unique():\n",
    "    subset = analysis_data[analysis_data['category_a'] == category]\n",
    "    sns.kdeplot(data=subset, x='satisfaction', ax=axes[1,2], label=category)\n",
    "axes[1,2].set_title('Satisfaction Distribution by Category')\n",
    "axes[1,2].legend()\n",
    "axes[1,2].grid(True, alpha=0.3)\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "\n",
    "# Summary statistics\n",
    "print(\"üìä Statistical Summary:\")\n",
    "print(f\"  ‚Ä¢ Strongest correlation: {correlation_matrix.abs().unstack().sort_values(ascending=False).iloc[1]:.3f}\")\n",
    "print(f\"  ‚Ä¢ Dataset variance explained by top 3 features: {correlation_matrix['target'].abs().nlargest(4)[1:4].sum():.2f}\")\n",
    "print(f\"  ‚Ä¢ Categories analyzed: {analysis_data['category_a'].nunique()} types, {analysis_data['category_b'].nunique()} groups\")"
   ]
  },
