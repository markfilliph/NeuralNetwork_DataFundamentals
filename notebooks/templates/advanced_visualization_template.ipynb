    "\n",
    "x = np.arange(len(quarterly_summary.index))\n",
    "width = 0.25\n",
    "\n",
    "bars1 = ax8.bar(x - width, quarterly_summary['revenue'], width, label='Revenue', color=BUSINESS_COLORS[0])\n",
