   "source": [
    "## Local File Upload Interface"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# File upload widgets\n",
    "upload_output = widgets.Output()\n",
    "\n",
    "file_upload = widgets.FileUpload(\n",
    "    accept='.csv,.xlsx,.xls,.txt,.tsv,.mdb,.accdb',\n",
    "    multiple=False,\n",
    "    description='Choose File',\n",
    "    style={'description_width': 'initial'}\n",
    ")\n",
    "\n",
    "dataset_name_input = widgets.Text(\n",
    "    placeholder='Enter dataset name (optional)',\n",
    "    description='Dataset Name:',\n",
    "    style={'description_width': 'initial'},\n",
    "    layout=widgets.Layout(width='400px')\n",
    ")\n",
    "\n",
    "dataset_description = widgets.Textarea(\n",
    "    placeholder='Enter dataset description (optional)',\n",
    "    description='Description:',\n",
    "    style={'description_width': 'initial'},\n",
    "    layout=widgets.Layout(width='400px', height='80px')\n",
    ")\n",
    "\n",
    "encrypt_checkbox = widgets.Checkbox(\n",
    "    value=True,\n",
    "    description='Encrypt file (recommended)',\n",
    "    style={'description_width': 'initial'}\n",
    ")\n",
    "\n",
    "upload_button = widgets.Button(\n",
    "    description='üì§ Upload to Platform',\n",
    "    button_style='success',\n",
    "    layout=widgets.Layout(width='200px'),\n",
    "    disabled=True\n",
    ")\n",
    "\n",
    "preview_button = widgets.Button(\n",
    "    description='üëÅÔ∏è Preview Data',\n",
    "    button_style='info',\n",
    "    layout=widgets.Layout(width='150px'),\n",
    "    disabled=True\n",
    ")\n",
    "\n",
    "def handle_file_upload_change(change):\n",
    "    \"\"\"Handle file selection.\"\"\"\n",
    "    global current_dataset\n",
    "    \n",
    "    with upload_output:\n",
    "        clear_output()\n",
    "        \n",
    "        if file_upload.value:\n",
    "            uploaded_file = list(file_upload.value.values())[0]\n",
    "            filename = uploaded_file['metadata']['name']\n",
    "            file_size = len(uploaded_file['content'])\n",
    "            \n",
    "            print(f\"üìÅ File selected: {filename}\")\n",
    "            print(f\"üìä File size: {file_size:,} bytes ({file_size/1024/1024:.2f} MB)\")\n",
    "            \n",
    "            # Validate file size (100MB limit)\n",
    "            if file_size > 100 * 1024 * 1024:\n",
    "                print(\"‚ùå File too large! Maximum size is 100MB.\")\n",
    "                upload_button.disabled = True\n",
    "                preview_button.disabled = True\n",
    "                return\n",
    "            \n",
    "            # Validate file extension\n",
    "            file_ext = Path(filename).suffix.lower()\n",
    "            supported_exts = ['.csv', '.xlsx', '.xls', '.txt', '.tsv', '.mdb', '.accdb']\n",
    "            \n",
    "            if file_ext not in supported_exts:\n",
    "                print(f\"‚ùå Unsupported file type: {file_ext}\")\n",
    "                print(f\"   Supported types: {', '.join(supported_exts)}\")\n",
    "                upload_button.disabled = True\n",
    "                preview_button.disabled = True\n",
    "                return\n",
    "            \n",
    "            # Store file data\n",
    "            current_dataset = {\n",
    "                'filename': filename,\n",
    "                'content': uploaded_file['content'],\n",
    "                'size': file_size,\n",
    "                'extension': file_ext\n",
    "            }\n",
    "            \n",
    "            # Auto-fill dataset name if not provided\n",
    "            if not dataset_name_input.value:\n",
    "                dataset_name_input.value = Path(filename).stem\n",
    "            \n",
    "            print(f\"‚úÖ File ready for upload: {file_ext} format\")\n",
    "            \n",
    "            # Enable buttons\n",
    "            preview_button.disabled = False\n",
    "            upload_button.disabled = not auth_token  # Only enable if authenticated\n",
    "            \n",
    "            if not auth_token:\n",
    "                print(\"‚ö†Ô∏è Please authenticate first to upload to platform\")\n",
    "        else:\n",
    "            upload_button.disabled = True\n",
    "            preview_button.disabled = True\n",
    "\n",
    "def handle_preview(button):\n",
    "    \"\"\"Preview the uploaded data.\"\"\"\n",
    "    with upload_output:\n",
    "        if not current_dataset:\n",
    "            print(\"‚ùå No file selected\")\n",
    "            return\n",
    "        \n",
    "        try:\n",
    "            print(f\"\\nüëÅÔ∏è Previewing: {current_dataset['filename']}\")\n",
    "            print(\"=\" * 50)\n",
    "            \n",
    "            # Create DataFrame based on file type\n",
    "            file_content = io.BytesIO(current_dataset['content'])\n",
    "            \n",
    "            if current_dataset['extension'] == '.csv':\n",
    "                df = pd.read_csv(file_content)\n",
    "            elif current_dataset['extension'] in ['.xlsx', '.xls']:\n",
    "                df = pd.read_excel(file_content)\n",
    "            elif current_dataset['extension'] in ['.txt', '.tsv']:\n",
    "                separator = '\\t' if current_dataset['extension'] == '.tsv' else ','\n",
    "                file_content.seek(0)\n",
    "                df = pd.read_csv(file_content, sep=separator)\n",
    "            else:\n",
    "                print(f\"‚ùå Preview not available for {current_dataset['extension']} files\")\n",
    "                return\n",
    "            \n",
    "            # Display basic info\n",
    "            print(f\"üìä Dataset Shape: {df.shape[0]:,} rows √ó {df.shape[1]} columns\")\n",
    "            print(f\"üíæ Memory Usage: {df.memory_usage(deep=True).sum() / 1024**2:.2f} MB\")\n",
    "            \n",
    "            # Show column info\n",
    "            print(f\"\\nüìã Columns ({len(df.columns)}):\")\n",
    "            for i, col in enumerate(df.columns[:10]):  # Show first 10 columns\n",
    "                dtype = str(df[col].dtype)\n",
    "                null_count = df[col].isnull().sum()\n",
    "                print(f\"  {i+1}. {col} ({dtype}) - {null_count} null values\")\n",
    "            \n",
    "            if len(df.columns) > 10:\n",
    "                print(f\"  ... and {len(df.columns) - 10} more columns\")\n",
    "            \n",
    "            # Show first few rows\n",
    "            print(f\"\\nüîç First 5 rows:\")\n",
    "            display(df.head())\n",
    "            \n",
    "            # Data quality summary\n",
    "            print(f\"\\nüìà Data Quality Summary:\")\n",
    "            print(f\"  ‚Ä¢ Missing values: {df.isnull().sum().sum():,} total\")\n",
    "            print(f\"  ‚Ä¢ Duplicate rows: {df.duplicated().sum():,}\")\n",
    "            print(f\"  ‚Ä¢ Numeric columns: {len(df.select_dtypes(include=[np.number]).columns)}\")\n",
    "            print(f\"  ‚Ä¢ Text columns: {len(df.select_dtypes(include=['object']).columns)}\")\n",
    "            print(f\"  ‚Ä¢ DateTime columns: {len(df.select_dtypes(include=['datetime64']).columns)}\")\n",
    "            \n",
    "        except Exception as e:\n",
    "            print(f\"‚ùå Preview error: {str(e)}\")\n",
    "            print(\"   File may be corrupted or in an unsupported format\")\n",
    "\n",
    "def handle_platform_upload(button):\n",
    "    \"\"\"Upload file to the platform API.\"\"\"\n",
    "    with upload_output:\n",
    "        if not current_dataset:\n",
    "            print(\"‚ùå No file selected\")\n",
    "            return\n",
    "        \n",
    "        if not auth_token:\n",
    "            print(\"‚ùå Please authenticate first\")\n",
    "            return\n",
    "        \n",
    "        try:\n",
    "            print(f\"\\nüì§ Uploading {current_dataset['filename']} to platform...\")\n",
    "            \n",
    "            # Prepare file for upload\n",
    "            files = {\n",
    "                'file': (current_dataset['filename'], current_dataset['content'])\n",
    "            }\n",
    "            \n",
    "            # Prepare form data\n",
    "            data = {\n",
    "                'encrypt': str(encrypt_checkbox.value).lower()\n",
    "            }\n",
    "            \n",
    "            if dataset_name_input.value:\n",
    "                data['name'] = dataset_name_input.value\n",
    "            \n",
    "            if dataset_description.value:\n",
    "                data['description'] = dataset_description.value\n",
    "            \n",
    "            # Upload headers (remove Content-Type for multipart)\n",
    "            upload_headers = {'Authorization': f\"Bearer {auth_token}\"}\n",
    "            \n",
    "            response = requests.post(\n",
    "                f\"{API_BASE_URL}/data/upload\",\n",
    "                files=files,\n",
    "                data=data,\n",
    "                headers=upload_headers\n",
    "            )\n",
    "            \n",
    "            if response.status_code == 200:\n",
    "                result = response.json()\n",
    "                print(\"‚úÖ Upload successful!\")\n",
    "                print(f\"üìä Dataset ID: {result['dataset_id']}\")\n",
    "                print(f\"üìÅ Dataset Name: {result['name']}\")\n",
    "                print(f\"üîê Encrypted: {result['is_encrypted']}\")\n",
    "                print(f\"üìÖ Uploaded: {result['created_at']}\")\n",
    "                \n",
    "                # Store dataset info for later use\n",
    "                uploaded_files[result['dataset_id']] = {\n",
    "                    'name': result['name'],\n",
    "                    'id': result['dataset_id'],\n",
    "                    'uploaded_at': result['created_at'],\n",
    "                    'encrypted': result['is_encrypted']\n",
    "                }\n",
    "                \n",
    "                print(f\"\\nüéØ Use this dataset ID in analysis notebooks: {result['dataset_id']}\")\n",
    "                \n",
    "            else:\n",
    "                error_data = response.json() if response.headers.get('content-type') == 'application/json' else {}\n",
    "                print(f\"‚ùå Upload failed: {error_data.get('detail', 'Upload error')}\")\n",
    "                \n",
    "        except requests.exceptions.ConnectionError:\n",
    "            print(\"‚ùå Could not connect to API. Make sure the server is running.\")\n",
    "        except Exception as e:\n",
    "            print(f\"‚ùå Upload error: {str(e)}\")\n",
    "\n",
    "# Connect event handlers\n",
    "file_upload.observe(handle_file_upload_change, names='value')\n",
    "preview_button.on_click(handle_preview)\n",
    "upload_button.on_click(handle_platform_upload)\n",
    "\n",
    "# Display upload interface\n",
    "upload_box = widgets.VBox([\n",
    "    widgets.HTML(\"<h3>üìÅ File Upload Interface</h3>\"),\n",
    "    file_upload,\n",
    "    dataset_name_input,\n",
    "    dataset_description,\n",
    "    encrypt_checkbox,\n",
    "    widgets.HBox([preview_button, upload_button]),\n",
    "    upload_output\n",
    "])\n",
    "\n",
    "display(upload_box)"
   ]
  },
  {
