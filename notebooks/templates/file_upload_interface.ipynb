   "source": [
    "## Dataset Management"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Dataset management widgets\n",
    "management_output = widgets.Output()\n",
    "\n",
    "list_datasets_button = widgets.Button(\n",
    "    description='üìã List My Datasets',\n",
    "    button_style='info',\n",
    "    layout=widgets.Layout(width='200px')\n",
    ")\n",
    "\n",
    "dataset_dropdown = widgets.Dropdown(\n",
    "    options=[],\n",
    "    description='Select Dataset:',\n",
    "    style={'description_width': 'initial'},\n",
    "    layout=widgets.Layout(width='400px')\n",
    ")\n",
    "\n",
    "dataset_info_button = widgets.Button(\n",
    "    description='‚ÑπÔ∏è Dataset Info',\n",
    "    button_style='primary',\n",
    "    layout=widgets.Layout(width='150px')\n",
    ")\n",
    "\n",
    "download_button = widgets.Button(\n",
    "    description='‚¨áÔ∏è Download',\n",
    "    button_style='warning',\n",
    "    layout=widgets.Layout(width='150px')\n",
    ")\n",
    "\n",
    "def handle_list_datasets(button):\n",
    "    \"\"\"List all user datasets.\"\"\"\n",
    "    with management_output:\n",
    "        clear_output()\n",
    "        \n",
    "        if not auth_token:\n",
    "            print(\"‚ùå Please authenticate first\")\n",
    "            return\n",
    "        \n",
    "        try:\n",
    "            response = requests.get(\n",
    "                f\"{API_BASE_URL}/data/list\",\n",
    "                headers={'Authorization': f\"Bearer {auth_token}\"}\n",
    "            )\n",
    "            \n",
    "            if response.status_code == 200:\n",
    "                datasets = response.json()\n",
    "                print(f\"üìä Found {len(datasets)} datasets:\\n\")\n",
    "                \n",
    "                if datasets:\n",
    "                    # Update dropdown options\n",
    "                    options = [(f\"{ds['name']} ({ds['dataset_id'][:8]}...)\", ds['dataset_id']) for ds in datasets]\n",
    "                    dataset_dropdown.options = options\n",
    "                    \n",
    "                    # Display dataset table\n",
    "                    datasets_df = pd.DataFrame(datasets)\n",
    "                    if 'created_at' in datasets_df.columns:\n",
    "                        datasets_df['created_at'] = pd.to_datetime(datasets_df['created_at']).dt.strftime('%Y-%m-%d %H:%M')\n",
    "                    \n",
    "                    display(datasets_df[['name', 'dataset_id', 'file_size', 'is_encrypted', 'created_at']])\n",
    "                else:\n",
    "                    print(\"No datasets found. Upload some data first!\")\n",
    "                    \n",
    "            else:\n",
    "                error_data = response.json() if response.headers.get('content-type') == 'application/json' else {}\n",
    "                print(f\"‚ùå Failed to list datasets: {error_data.get('detail', 'Error')}\")\n",
    "                \n",
    "        except requests.exceptions.ConnectionError:\n",
    "            print(\"‚ùå Could not connect to API\")\n",
    "        except Exception as e:\n",
    "            print(f\"‚ùå Error: {str(e)}\")\n",
    "\n",
    "def handle_dataset_info(button):\n",
    "    \"\"\"Get detailed dataset information.\"\"\"\n",
    "    with management_output:\n",
    "        if not dataset_dropdown.value:\n",
    "            print(\"‚ùå Please select a dataset first\")\n",
    "            return\n",
    "        \n",
    "        try:\n",
    "            dataset_id = dataset_dropdown.value\n",
    "            response = requests.get(\n",
    "                f\"{API_BASE_URL}/data/{dataset_id}/info\",\n",
    "                headers={'Authorization': f\"Bearer {auth_token}\"}\n",
    "            )\n",
    "            \n",
    "            if response.status_code == 200:\n",
    "                info = response.json()\n",
    "                print(f\"\\nüìä Dataset Information: {info['name']}\")\n",
    "                print(\"=\" * 50)\n",
    "                print(f\"ID: {info['dataset_id']}\")\n",
    "                print(f\"Size: {info['file_size']:,} bytes\")\n",
    "                print(f\"Encrypted: {info['is_encrypted']}\")\n",
    "                print(f\"Created: {info['created_at']}\")\n",
    "                print(f\"Owner: {info['owner_id']}\")\n",
    "                \n",
    "                if 'metadata' in info and info['metadata']:\n",
    "                    print(f\"\\nMetadata:\")\n",
    "                    for key, value in info['metadata'].items():\n",
    "                        print(f\"  {key}: {value}\")\n",
    "                        \n",
    "            else:\n",
    "                error_data = response.json() if response.headers.get('content-type') == 'application/json' else {}\n",
